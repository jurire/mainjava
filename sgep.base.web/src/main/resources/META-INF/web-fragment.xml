<?xml version="1.0" encoding="UTF-8"?>
<web-fragment id="WebFragment_ID" version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd">
	<display-name>Sgepbaseweb</display-name>
	<name>Sgepbaseweb</name>
	
	<servlet>
		<servlet-name>BaseWebServlet</servlet-name>
		<servlet-class>gov.sgk.sgep.base.web.BaseWebServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>BaseWebServlet</servlet-name>
		<url-pattern>/base</url-pattern>
	</servlet-mapping>
	
</web-fragment>